#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t; cin>>t;
	while(t--)
	{
		int rows; cin>>rows;
		
		for(int i = 1, k = 0; i <= rows; ++i, k = 0)
    	{
        	for(int space = 1; space <= rows-i; ++space)
        	{
            	cout <<" ";
        	}

        	while(k != 2*i-1)
        	{
            	cout << "*";
            	++k;
        	}
        	cout << endl;
    	}	    
		cout<<endl;
	}
}
