#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;cin>>t;
    while(t--)
    {
        string string1,string2; cin>>string1>>string2;
        sort(string1.begin(),string1.end());
        sort(string2.begin(),string2.end());

        if(string1==string2)
            cout<<"YES\n";
        else
            cout<<"NO\n";    
    }
}
