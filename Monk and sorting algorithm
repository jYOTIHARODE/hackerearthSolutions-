#include<bits/stdc++.h>
using namespace std;
int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int T; cin>>T;
	int arr[T];
	for(int i=0;i<T;i++) cin>>arr[i];
	
	sort(arr,arr+T,greater <int>());
	for(int i=0;i<T;i++) cout<<arr[i]<<" ";
	cout<<"\n";
	sort(arr,arr+T);
	for(int i=0;i<T;i++) cout<<arr[i]<<" ";
	return 0;
}
